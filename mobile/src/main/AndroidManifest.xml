<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="finalproject.ece558.edu.pdx.ece.brailleblackjack" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LearnBrailleActivity"
            android:label="Learn Braille" >
        </activity>
        <activity
            android:name=".PlayBlackJackActivity"
            android:label="Play Black Jack" >
        </activity>
        <activity
            android:name=".AboutActivity"
            android:label="About Braille Black Jack" >
        </activity>
    </application>



</manifest>
